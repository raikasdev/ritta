{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
			"linebreak-style": "off",
			"global-require": "off",
      "no-param-reassign": "off"
    },
    "ignorePatterns": ["assets/js/*"]
  }